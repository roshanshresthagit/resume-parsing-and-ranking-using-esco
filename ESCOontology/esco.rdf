    <rdfs:subClassOf rdf:resource="http://data.europa.eu/esco/model#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/esco/model"/>
    <ns2:disjointWith rdf:resource="http://data.europa.eu/esco/model#SimpleConcept"/>
